{
  "version": 2,
  "functions": {
    "api/health.ts": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "node_modules/{atomic-sleep,colorette,dateformat,end-of-stream,fast-copy,fast-safe-stringify,help-me,joycon,minimist,on-exit-leak-free,once,pino-abstract-transport,pino-pretty,pump,secure-json-parse,sonic-boom,split2,strip-json-comments,wrappy}/**"
    }
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
