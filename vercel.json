{
  "version": 2,
  "functions": {
    "api/health.ts": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles": "node_modules/pino-pretty/**"
    }
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
